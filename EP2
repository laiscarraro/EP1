bool temSub(NO* inicio) {
    while(inicio) {
        if(inicio->tipo == 2) return true;
        inicio = inicio->prox;
    }
    return false;
}

// o EP consiste em implementar esta funcao
/*NO* listarChaves(NO* entrada) {
